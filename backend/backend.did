type Task = 
 record {
   completed: bool;
   icon: text;
   id: nat;
   reward: nat;
   "text": text;
 };
type Result = 
 variant {
   err: text;
   ok: Task;
 };
service : {
  addTask: (text, text, nat) -> (Result);
  completeTask: (nat) -> (Result);
  getProgress: () -> (record {
                        completed: nat;
                        total: nat;
                      }) query;
  getTasks: () -> (vec Task);
}
